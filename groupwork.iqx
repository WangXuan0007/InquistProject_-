<trial ul>
/ correctmessage = (correctmsg,500)
/ errormessage = (errormsg,500)
/ validresponse = ("F","J")
/ correctresponse = ("F")
/ stimulustimes = [1=fixation;500=cue1;1000=sti1]
</trial>

<trial ur>
/ correctmessage = (correctmsg,500)
/ errormessage = (errormsg,500)
/ validresponse = ("F","J")
/ correctresponse = ("J")
/ stimulustimes = [1=fixation;500=cue2;1000=sti2]
</trial>

<trial dl>
/ correctmessage = (correctmsg,500)
/ errormessage = (errormsg,500)
/ validresponse = ("F","J")
/ correctresponse = ("F")
/ stimulustimes = [1=fixation;500=cue2;1000=sti3]
