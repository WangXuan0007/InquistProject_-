<trial instruction>
 	/validresponse = (anyresponse)
 	/stimulusframes = [1=instruction]
 	/recorddata = false
</trial>
 
<trial bye>
 	/validresponse = (anyresponse)
 	/stimulusframes = [1=end]
 	/recorddata = false
</trial>
 
<block instructionblk>
 	/screencolor = (0,0,0)
 	/trials = [1 = instruction]
</block>
 
<block byeblk>
 	/screencolor = (0,0,0)
 	/trials = [1 = bye]
</block>

<block all>
/ screencolor = (0,0,0)
/ blockfeedback = (meanlatency,correct)
/ trials = [1-96 = noreplcerepeat(ul,ur,dl,dr)]
</trial>

